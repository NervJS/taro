{
  "name": "@tarojs/taroize",
  "version": "3.6.31",
  "description": "转换原生微信小程序代码为 Taro 代码",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w --pretty",
    "lint": "eslint src --ext .js --ext .jsx --ext .ts --ext .tsx",
    "format": "prettier --write \"{src, __tests__}/**/*.{ts,tsx,js,jsx}\" && eslint src __tests__ --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:cov": "jest --coverage",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:ci": "cross-env NODE_ENV=test jest --ci -i --coverage --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NervJS/taro/tree/master/packages/taroize"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/taroize/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/util.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/__tests__/*",
      "/src/vue.ts"
    ],
    "transformIgnorePatterns": [
      "@taro-helper/node_modules/"
    ]
  },
  "author": "yuche",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.21.4",
    "@babel/generator": "^7.21.4",
    "@babel/parser": "^7.23.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-flow": "^7.22.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/template": "7.21.9",
    "@babel/traverse": "^7.21.4",
    "@babel/types": "^7.21.4",
    "@tarojs/helper": "workspace:*",
    "himalaya-wxml": "^1.1.0",
    "html": "^1.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.4"
  }
}
