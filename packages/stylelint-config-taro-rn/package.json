{
  "name": "stylelint-config-taro-rn",
  "version": "4.0.8-beta.0",
  "description": "Shareable stylelint config for React Native CSS modules",
  "author": "O2Team",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "keywords": [
    "stylelint",
    "stylelint-config",
    "react-native",
    "css-modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "stylelint-taro-rn": "workspace:*"
  },
  "devDependencies": {
    "stylelint": "^16.4.0"
  },
  "peerDependencies": {
    "stylelint": "^16",
    "stylelint-taro-rn": "workspace:*"
  }
}
