{
  "name": "@tarojs/components",
  "version": "3.5.4-canary.1",
  "description": "Taro 组件库",
  "browser": "dist/index.js",
  "main:h5": "dist/index.js",
  "main": "dist/index.js",
  "module": "dist/index.cjs.js",
  "types": "types/index.d.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "dist",
    "loader",
    "mini",
    "types",
    "virtual-list",
    "vue3.d.ts"
  ],
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s build:components build:library",
    "build:components": "stencil build --config stencil.config.ts",
    "build:library": "pnpm --filter @tarojs/components-library-react --filter @tarojs/components-library-vue2 --filter @tarojs/components-library-vue3 run build:ci",
    "dev:components": "cross-env NODE_ENV=development pnpm run build:components --watch",
    "dev:library-react": "cross-env NODE_ENV=development pnpm --filter @tarojs/components-library-react run build:ci --watch",
    "dev:library-vue2": "cross-env NODE_ENV=development pnpm --filter @tarojs/components-library-vue2 run build:ci --watch",
    "dev:library-vue3": "cross-env NODE_ENV=development pnpm --filter @tarojs/components-library-vue3 run build:ci --watch",
    "test": "cross-env NODE_ENV=test stencil test --spec --e2e",
    "test.watch": "cross-env NODE_ENV=test stencil test --spec --e2e --watchAll",
    "test:ci": "cross-env NODE_ENV=test stencil test --spec --e2e --no-build",
    "test:coverage": "cross-env NODE_ENV=test stencil test --spec --e2e --coverage",
    "sync:types": "ts-node scripts/json-schema-to-types.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tarojs/router": "workspace:*",
    "@tarojs/taro": "workspace:*",
    "classnames": "^2.2.5",
    "hls.js": "^1.1.5",
    "intersection-observer": "^0.7.0",
    "resolve-pathname": "^3.0.0",
    "swiper": "6.8.0",
    "weui": "^1.1.2"
  },
  "devDependencies": {
    "@stencil/core": "^2.18.1",
    "@stencil/react-output-target": "0.3.1",
    "@stencil/sass": "^2.0.0",
    "@stencil/vue-output-target": "0.6.2",
    "@types/node": "^14.14.31",
    "css-loader": "3.4.2",
    "humps": "^2.0.1",
    "jquery": "^3.4.1",
    "karmatic": "^2.1.0",
    "lodash": "^4.17.21",
    "miniapp-types": "latest",
    "simulant": "^0.2.2",
    "style-loader": "1.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "4.46.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-native": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
