{
  "name": "@tarojs/plugin-platform-h5",
  "version": "3.6.24",
  "description": "Web 端平台插件",
  "author": "ZakaryCode",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "taro"
  ],
  "files": [
    "build",
    "dist",
    "index.js",
    "types"
  ],
  "sideEffects": [
    "./dist/runtime/index.js",
    "*.scss",
    "*.css"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "scripts": {
    "prebuild": "run-s clean generate:desc",
    "build": "pnpm run rollup --environment NODE_ENV:production",
    "clean": "rimraf dist",
    "dev": "pnpm generate:desc && pnpm run rollup --environment NODE_ENV:development -w",
    "generate:desc": "pnpm run tsx ./build/definition-json/index.ts",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "tsx": "tsx"
  },
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "dependencies": {
    "@tarojs/components": "workspace:*",
    "@tarojs/components-react": "workspace:*",
    "@tarojs/router": "workspace:*",
    "@tarojs/runtime": "workspace:*",
    "@tarojs/service": "workspace:*",
    "@tarojs/shared": "workspace:*",
    "@tarojs/taro-h5": "workspace:*",
    "babel-plugin-transform-taroapi": "workspace:*",
    "change-case": "^4.1.2",
    "lodash-es": "4.17.21",
    "resolve": "^1.22.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@tarojs/taro": "workspace:*",
    "@types/node": "^14.14.31",
    "rollup": "^3.8.1",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-ts": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
