  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-environment-node": "^29.5.0",
