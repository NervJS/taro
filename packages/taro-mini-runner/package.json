    "resolve": "^1.11.1",
    "sass-loader": "^8.0.2",
    "sax": "1.2.4",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "tapable": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "^4.1.0",
    "vm2": "^3.8.4",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "4.42.0",
    "webpack-chain": "4.9.0",
    "webpack-format-messages": "^2.0.5",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@tarojs/components": "3.0.0-rc.3",
