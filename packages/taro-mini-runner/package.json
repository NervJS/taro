{
  "name": "@tarojs/mini-runner",
  "version": "3.0.0-rc.3",
  "description": "Mini app runner for taro",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run prod",
    "dev": "tsc -w",
    "prod": "tsc",
    "clean": "rimraf dist",
    "lint": "tslint src/**/*.ts --fix",
    "lint:typecheck": "tslint -p tsconfig.json src/**/*.ts --fix",
    "test": "env NODE_ENV=jest jest",
    "test:dev": "env NODE_ENV=jest jest --watch",
    "test:coverage": "env NODE_ENV=jest jest --coverage",
    "updateSnapshot": "env NODE_ENV=jest jest --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "files": [
    "index.js",
    "dist"
  ],
  "keywords": [
    "taro"
  ],
  "author": "luckyadam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "homepage": "https://github.com/NervJS/taro#readme",
  "dependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.3",
    "@tarojs/helper": "3.0.0-rc.3",
    "@tarojs/runner-utils": "3.0.0-rc.3",
    "@tarojs/runtime": "3.0.0-rc.3",
    "@tarojs/shared": "3.0.0-rc.3",
    "@tarojs/taro": "3.0.0-rc.3",
    "@tarojs/taro-loader": "3.0.0-rc.3",
    "babel-loader": "8.0.6",
    "babel-types": "^6.26.0",
    "copy-webpack-plugin": "^5.0.3",
    "css": "2.2.4",
    "css-loader": "^3.0.0",
    "css-what": "3.2.0",
    "csso-webpack-plugin": "^1.0.0-beta.12",
    "file-loader": "^6.0.0",
    "fs-extra": "^8.0.1",
    "jsdom": "^15.2.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.11",
    "micromatch": "^4.0.2",
    "mini-css-extract-plugin": "0.8.0",
    "miniprogram-simulate": "^1.1.5",
    "node-sass": "^4.12.0",
    "ora": "^3.4.0",
    "postcss-import": "12.0.1",
