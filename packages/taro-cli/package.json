{
  "name": "@tarojs/cli",
  "version": "3.6.15",
  "description": "cli tool for taro",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci -i",
    "test:dev": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "updateSnapshot": "cross-env NODE_ENV=test jest --updateSnapshot",
    "build": "pnpm run clean && pnpm run prod",
    "dev": "tsc -w",
    "prod": "tsc",
    "clean": "rimraf dist"
  },
  "files": [
    "bin",
    "__tests__",
    "scripts",
    "src",
    "dist",
    "templates",
    "index.js",
    "global.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "bin": {
    "taro": "bin/taro"
  },
  "keywords": [
    "taro",
    "weapp"
  ],
  "engines": {
    "node": ">=12"
  },
  "author": "O2Team",
  "license": "MIT",
  "dependencies": {
    "@tarojs/helper": "workspace:*",
    "@tarojs/service": "workspace:*",
    "@tarojs/shared": "workspace:*",
    "@tarojs/plugin-doctor": "^0.0.11",
    "adm-zip": "^0.4.13",
    "cli-highlight": "^2.1.11",
    "download-git-repo": "^2.0.0",
    "envinfo": "^7.8.1",
    "eslint": "^8.12.0",
    "glob": "^7.1.2",
    "inquirer": "^8.0.0",
    "joi": "^17.6.0",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.21",
    "mem-fs": "^2.3.0",
    "mem-fs-editor": "^9.6.0",
    "minimist": "^1.2.8",
    "npm-check": "^6.0.1",
    "ora": "^5.0.0",
    "request": "^2.88.0",
    "semver": "^7.3.8",
    "validate-npm-package-name": "^5.0.0",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "babel-jest": "^29.5.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-environment-node": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.4"
  }
}
