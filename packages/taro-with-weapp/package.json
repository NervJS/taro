{
  "name": "@tarojs/with-weapp",
  "version": "4.0.8-beta.0",
  "description": "taroize 之后的运行时",
  "author": "O2Team",
  "license": "MIT",
  "main": "index.js",
  "exports": {
    ".": "./dist/index.esm.js"
  },
  "files": [
    "dist",
    "src",
    "index.js",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NervJS/taro/tree/master/packages/taro-with-weapp"
  },
  "scripts": {
    "prod": "pnpm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "tsc": "tsc",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" && eslint src --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "test": "jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci -i --silent",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@tarojs/runtime": "workspace:*",
    "@tarojs/taro": "workspace:*"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
