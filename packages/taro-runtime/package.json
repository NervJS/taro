{
  "name": "@tarojs/runtime",
  "version": "3.7.0-beta.3",
  "description": "taro runtime for mini apps.",
  "main:h5": "dist/runtime.h5.js",
  "main": "dist/runtime.esm.js",
  "module": "dist/runtime.esm.js",
  "types": "./dist/runtime.esm.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "pnpm run rollup",
    "dev": "pnpm run rollup --environment TARGET:esm -w",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs",
    "test": "jest",
    "test:ci": "jest --ci -i --coverage false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NervJS/taro/tree/master/packages/taro-runtime"
  },
  "author": "yuche",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tarojs/shared": "workspace:*",
    "lodash-es": "4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@vue/runtime-core": "3.2.41",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-environment-node": "^29.5.0",
    "rollup": "^3.8.1",
    "rollup-plugin-ts": "^3.0.2",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^4.7.4"
  }
}
