{
  "name": "@tarojs/utils",
  "version": "2.2.14",
  "description": "Utils for Taro",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "keywords": [
    "taro"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "luckyadam",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
