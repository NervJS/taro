{
  "name": "@tarojs/router",
  "version": "4.0.8-beta.0",
  "description": "Taro-router",
  "author": "O2Team",
  "license": "MIT",
  "browser": "dist/index.js",
  "main:h5": "dist/index.esm.js",
  "main": "dist/index.js",
  "module": "dist/index.cjs.js",
  "typings": "dist/index.esm.d.ts",
  "files": [
    "dist",
    "types",
    "index.js"
  ],
  "sideEffects": [],
  "scripts": {
