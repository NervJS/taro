  },
  "dependencies": {
    "dingtalk-jsapi": "~2.15.2",
    "history": "^5.3.0",
    "mobile-detect": "^1.4.2",
    "query-string": "^9.0.0",
    "tslib": "^2.6.2",
    "universal-router": "^9.2.0"
  },
  "devDependencies": {
    "@tarojs/components": "workspace:*",
    "@tarojs/runtime": "workspace:*",
    "@tarojs/taro": "workspace:*",
    "@tarojs/shared": "workspace:*"
  },
  "peerDependencies": {
    "@tarojs/runtime": "workspace:*",
    "@tarojs/taro": "workspace:*",
    "@tarojs/shared": "workspace:*"
