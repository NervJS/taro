<div>
    <input
            id="{{ id }}"
            class="{{ cls }}"
            type="{{ realType }}"
            placeholder="{{ placeholder }}"
            autofocus="{{ focus }}"
            enterkeytype="{{ confirmType }}"
            maxlength="{{ maxlength }}"
            disabled="{{ disabled }}"

            @change="onInput"
            @focus="onFocus"
            @blur="onBlur"
            @enterkeyclick="onEnterKeyClick"
            >
    </input>
</div>

