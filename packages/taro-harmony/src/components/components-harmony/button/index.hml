<div>
  <button
    id="{{id}}"
    class="{{cls}} {{clsType}} {{clsSize}} {{clsPlain}} {{clsDisable}} {{clsDisableType}} {{clsLoading}} {{clsHover}}"
    type="{{harmonyType}}"
    disabled="{{disabled}}"
    waiting="{{loading}}"
    @click="onClick"
    @touchstart="onTouchStart"
    @touchend="onTouchEnd"
  >{{value}}</button>
</div>
