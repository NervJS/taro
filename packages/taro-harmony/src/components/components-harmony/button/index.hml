<div>
  <button
    id="{{id}}"
    class="{{cls}} {{clsType}} {{clsSize}} {{clsPlain}} {{clsDisable}} {{clsDisableType}} {{clsHover}}"
    type="{{harmonyType}}"
    disabled="{{disabled}}"
    waiting="{{loading}}"
    @touchstart="{{onTouchStart}}"
    @touchend="{{onTouchEnd}}"
  >{{value}}</button>
</div>
