<div class="switch">
  <input if="{{type === 'checkbox'}}" id="{{id}}" class="{{cls}}" type="checkbox" checked="{{checked}}"
    disabled="{{disabled}}" @change="onChange"></input>
  <switch else id="{{id}}" class="{{cls}}" checked="{{checked}}" disabled="{{disabled}}" showtext="{{showtext}}"
    texton="{{texton}}" textoff="{{textoff}}" @change="onChange">
  </switch>
</div>
